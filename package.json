{
  "name": "network-analysis",
  "version": "1.0.0",
  "license": "MIT",
  "author": "shulan <adengminjie@163.com>",
  "description": "a network analysis tool",
  "private": true,
  "main": "./public/electron.js",
  "homepage": ".",
  "dependencies": {
    "chrome-remote-interface": "^0.27.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build --em.main public/electron.js public/main",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "estart": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^20.44.4",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1"
  },
  "build": {
    "appId": "com.network-analysis",
    "productName": "network-analysis",
    "asar": false,
    "directories": {
      "buildResources": "assets"
    },
    "files": [],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  }
}
